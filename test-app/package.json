{
  "name": "test-app",
  "version": "5.0.0",
  "description": "A content-editable component for ember-cli that just works.",
  "keywords": [
    "ember",
    "ember-cli",
    "contenteditable",
    "content",
    "editable",
    "textarea",
    "auto resize",
    "resize",
    "resizing"
  ],
  "repository": "https://github.com/st-h/ember-content-editable",
  "license": "MIT",
  "author": "st-h",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "format": "prettier . --cache --write",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\" --prefixColors auto",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\" --prefixColors auto && pnpm format",
    "lint:format": "prettier . --cache --check",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\" --prefixColors auto",
    "test:ember": "ember test --test-port=0",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "@babel/core": "^7.29.0",
    "ember-template-imports": "^4.4.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.6",
    "@babel/plugin-proposal-decorators": "^7.29.0",
    "@ember/optional-features": "^3.0.0",
    "@ember/test-helpers": "^5.4.1",
    "@embroider/macros": "^1.19.7",
    "@embroider/test-setup": "^4.0.0",
    "@eslint/js": "^9.39.2",
    "@glimmer/component": "^2.0.0",
    "@glimmer/tracking": "^1.1.2",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^9.2.1",
    "coveralls": "^3.1.1",
    "ember-auto-import": "^2.12.0",
    "ember-cli": "~6.10.2",
    "ember-cli-babel": "^8.3.1",
    "ember-cli-clean-css": "^3.0.0",
    "ember-cli-code-coverage": "^3.1.0",
    "ember-cli-dependency-checker": "^3.3.3",
    "ember-cli-deprecation-workflow": "^4.0.0",
    "ember-cli-github-pages": "^0.2.2",
    "ember-cli-htmlbars": "^7.0.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-terser": "^4.0.2",
    "ember-content-editable": "4.0.0",
    "ember-content-editable-modifier": "^4.0.3",
    "ember-load-initializers": "^3.0.1",
    "ember-maybe-import-regenerator-for-testing": "^1.0.0",
    "ember-page-title": "^9.0.3",
    "ember-qunit": "^9.0.4",
    "ember-resolver": "^13.1.1",
    "ember-source": "^6.10.1",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^7.9.3",
    "ember-try": "^4.0.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-ember": "^12.7.5",
    "eslint-plugin-n": "^17.23.2",
    "eslint-plugin-qunit": "^8.2.6",
    "globals": "^17.3.0",
    "loader.js": "^4.7.0",
    "prettier": "^3.8.1",
    "prettier-plugin-ember-template-tag": "^2.1.3",
    "qunit": "^2.25.0",
    "qunit-dom": "^3.5.0",
    "stylelint": "^17.2.0",
    "stylelint-config-standard": "^40.0.0",
    "webpack": "^5.105.2"
  },
  "engines": {
    "node": ">= 20.19"
  },
  "ember": {
    "edition": "octane"
  },
  "private": true
}
