{
  "name": "ember-content-editable",
  "version": "5.0.0",
  "private": true,
  "repository": "",
  "license": "MIT",
  "author": "",
  "packageManager": "pnpm@10.29.2",
  "scripts": {
    "build": "pnpm --filter ./ember-content-editable run build",
    "lint": "pnpm -r run lint",
    "lint:fix": "pnpm -r run lint:fix",
    "prepare": "pnpm build",
    "start": "concurrently 'npm:start:*' --restart-after 5000 --prefix-colors cyan,white,yellow",
    "start:addon": "pnpm --filter ./ember-content-editable run start",
    "start:test-app": "pnpm --filter ./test-app run start",
    "test": "pnpm -r run test"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "prettier": "^3.8.1"
  }
}
