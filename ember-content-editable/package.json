{
  "name": "ember-content-editable",
  "version": "5.0.0",
  "description": "A content-editable component for ember-cli that just works.",
  "keywords": [
    "ember-addon",
    "ember",
    "ember-cli",
    "contenteditable",
    "content",
    "editable",
    "textarea",
    "auto resize",
    "resize",
    "resizing"
  ],
  "repository": "https://github.com/st-h/ember-content-editable",
  "license": "MIT",
  "author": "st-h",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "rollup --config",
    "prepack": "rollup --config",
    "start": "rollup --config --watch"
  },
  "dependencies": {
    "@embroider/addon-shim": "^1.10.2",
    "ember-content-editable-modifier": "^4.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.29.0",
    "@embroider/addon-dev": "^8.3.0",
    "@rollup/plugin-babel": "^6.1.0",
    "rollup": "^4.57.1",
    "rollup-plugin-copy": "^3.5.0"
  },
  "engines": {
    "node": "14.* || 16.* || >= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "app-js": {
      "./components/content-editable.js": "./dist/_app_/components/content-editable.js"
    },
    "main": "addon-main.cjs",
    "type": "addon",
    "version": 2
  },
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js",
    "./addon-main.js": "./addon-main.cjs"
  },
  "files": [
    "addon-main.cjs",
    "dist"
  ]
}
